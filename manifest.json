{
  "manifest_version": 2,
  "name": "Badge Finder",
  "version": "0.1", 
  "background": {
  "scripts": ["background.js"]
  },
  "content_scripts": [
  {
    "matches": [
      "https://scholar.google.de/*",
      "https://scholar.google.com/*"
    ],
    "js": ["jquery-3.1.1.min.js","content.js"]
  }
],
  "browser_action": {
    "default_icon": "icon.png"
  }
}